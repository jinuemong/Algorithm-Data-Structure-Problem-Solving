   "source": [
    "a = 10\n",
    "b = 12 \n",
    "\n",
    "# 최대 공약수 구하기\n",
    "for i in range(min(a,b),0,-1):\n",
    "    if a%i == 0 and b % i == 0:\n",
    "        print(i)\n",
    "        break\n",
    "\n",
    "# 최소 공배수 구하기\n",
    "for i in range(max(a,b),a*b+1):\n",
    "    if i%a==0 and i%b == 0:\n",
    "        print(i)\n",
    "        break\n",
    "\n"
